<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="@dimen/space_40"
    android:gravity="center">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="@dimen/space_32"
        android:layout_height="@dimen/space_32"
        android:layout_weight="1"
        android:background="@drawable/bg_face"
        android:contentDescription="@null"
        android:padding="@dimen/space_2"
        app:srcCompat="@drawable/ic_state_face"
        app:tint="@color/face_tab_color" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="@dimen/space_32"
        android:layout_height="@dimen/space_32"
        android:layout_weight="1"
        android:background="@drawable/bg_face"
        android:contentDescription="@null"
        android:padding="@dimen/space_2"
        app:srcCompat="@drawable/ic_state_bell"
        app:tint="@color/face_tab_color" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="@dimen/space_32"
        android:layout_height="@dimen/space_32"
        android:layout_weight="1"
        android:background="@drawable/bg_face"
        android:contentDescription="@null"
        android:padding="@dimen/space_2"
        app:srcCompat="@drawable/ic_state_animal"
        app:tint="@color/face_tab_color" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="@dimen/space_32"
        android:layout_height="@dimen/space_32"
        android:layout_weight="1"
        android:background="@drawable/bg_face"
        android:contentDescription="@null"
        android:padding="@dimen/space_2"
        app:srcCompat="@drawable/ic_state_building" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="@dimen/space_32"
        android:layout_height="@dimen/space_32"
        android:layout_weight="1"
        android:background="@drawable/bg_face"
        android:contentDescription="@null"
        android:padding="@dimen/space_2"
        app:srcCompat="@drawable/ic_state_characters"
        app:tint="@color/face_tab_color" />
</merge>